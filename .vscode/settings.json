{
  "editor.tabSize": 2,
  "editor.defaultFormatter": "numso.prettier-standard-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "svipas.prettier-plus"
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.eslintIntegration": true,
  "prettier.parser": "babel-ts",
  "prettier.stylelintIntegration": true,
  "prettier.tslintIntegration": true,
  "iisexpress.openInBrowser": "chrome",
  "editor.formatOnSave": true,
  "amVim.mimicVimSearchBehavior": false,
  "amVim.bindCtrlCommands": false,
  "npm.oldKeybindings.enable": true,
  "docker.languageserver.formatter.ignoreMultilineInstructions": true,
  "editor.formatOnPaste": false,
  "eslint.format.enable": true,
  "cloudcode.yaml.format.enable": true,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "ReactComponentContainerGenerator.componentFile.js": "js",
  "vsintellicode.features.apiExamplests": "enabled"
}
